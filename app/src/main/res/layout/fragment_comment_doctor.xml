<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    style="@style/SCREEN"
    tools:context="com.example.jpyou.ui.view.fragment.CommentDoctorFragment">


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="42dp"
            android:orientation="horizontal">

            <Button
                android:id="@+id/buttonBack_CommentDoctorFragment"
                android:layout_width="75dp"
                android:layout_height="wrap_content"
                android:text="Button" />

            <TextView
                android:id="@+id/textView5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="Đánh giá bác sĩ" />
        </LinearLayout>

        <TextView
            android:id="@+id/textView7"
            android:layout_width="match_parent"
            android:layout_height="68dp"
            android:text="Bác sĩ bạn từng khám" />

        <Spinner
            android:id="@+id/spinnerDoctor_CommentDoctorFragment"
            android:layout_width="match_parent"
            android:layout_height="59dp" />

        <TextView
            android:id="@+id/textViewNameDoctor_CommentDoctorFragment"
            android:layout_width="match_parent"
            android:layout_height="46dp"
            android:text="TextView" />

        <EditText
            android:id="@+id/editTextText_CommentDoctorFragment"
            android:layout_width="match_parent"
            android:layout_height="144dp"
            android:ems="10"
            android:inputType="text"
            android:text="Name" />

        <Button
            android:id="@+id/buttonConfirm_CommentDoctorFragment"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Button" />
    </LinearLayout>
</FrameLayout>