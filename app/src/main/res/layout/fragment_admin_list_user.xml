<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    style="@style/SCREEN"
    android:id="@+id/fragmnent_admin_list_user"
    tools:context="com.example.jpyou.ui.view.fragment.ListUserAdminFragment">


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="44dp"
            android:orientation="horizontal">

            <Button
                android:id="@+id/buttonBack_ListUserAdminFragment"
                android:layout_width="50dp"
                android:layout_height="40dp"
                android:background="@drawable/ic_back"
                app:backgroundTint="@null" />

            <SearchView
                android:id="@+id/searchView_ListUserAdminFragment"
                android:layout_width="match_parent"
                android:layout_height="match_parent" />

        </LinearLayout>

        <ListView
            android:id="@+id/listView_ListUserAdminFragment"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            style="@style/BUTTON"
            android:layout_margin="5dp"
            android:padding="5dp"/>
    </LinearLayout>
</FrameLayout>